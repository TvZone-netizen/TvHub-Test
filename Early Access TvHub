    Description = "Self Explanatory",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-625.295, 947.804, -1368.489)
    end
})

local Section4 = Tabs.Main:AddSection("B1C4")

Tabs.Main:AddButton({
    Title = "1 auto pass",
    Description = "Self Explanatory",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(85.467, -51, -1417.053)
    end
})

Tabs.Main:AddButton({
    Title = "Kusonoki Auto pass",
    Description = "Automatically burns cursed items",
    Callback = function()
        local cursedItems = workspace.GameInfo.PuzzleItems:GetChildren()
        local autoBurn = workspace.Well.Burner

        for _, item in ipairs(cursedItems) do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(item.Position)
            task.wait(0.3)
            fireproximityprompt(item.ProximityPrompt)
        end

        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(686.951, 26.722, 2252.857)
        task.wait(0.5)

        for _ = 1, 6 do
            fireproximityprompt(autoBurn.ProximityPrompt)
            task.wait(0.3)
        end
    end
})

Tabs.Main:AddButton({
    Title = "Heart Part",
    Description = "Auto Pass",
    Callback = function()
        local function checkHeart(index, position)
            while true do
                local heart = workspace.GameHearts:GetChildren()[index]
                if heart and heart:FindFirstChild("Destroyed") then
                    break
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(position)
                end
                task.wait(0.5)
            end
        end

        checkHeart(1, Vector3.new(3208.44, 148.64, 2187.78))
        checkHeart(3, Vector3.new(2806.88, 134.34, 2426.09))
        checkHeart(2, Vector3.new(2603.96, 146.41, 2409))
    end
})

Tabs.Main:AddSlider("Slider", {
    Title = "Increase WS (B1 Only)",
    Description = "Increase WalkSpeed",
    Default = 16,
    Min = 0,
    Max = 100,
    Rounding = 0,
    Callback = function(Value)
        local antiSpeed = game.Players.LocalPlayer.PlayerScripts:FindFirstChild("Anti-Speed")
        if antiSpeed then
            antiSpeed:Destroy()
        end
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

local toggleButton = Instance.new("TextButton")
toggleButton.Size = UDim2.new(0, 100, 0, 50)
toggleButton.Position = UDim2.new(0, 0, 0, 0)
toggleButton.Text = "Toggle UI"
toggleButton.Parent = game.Players.LocalPlayer.PlayerGui

local isOpen = true
toggleButton.MouseButton1Click:Connect(function()
    if isOpen then
        Window:Close()
    else
        Window:Open()
    end
    isOpen = not isOpen
end)
