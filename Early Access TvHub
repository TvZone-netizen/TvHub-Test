local NothingLibrary = loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/3345-c-a-t-s-u-s/NOTHING/main/source.lua'))()
local Windows = NothingLibrary.new({
    Title = "TvHub | Mimic",
    Description = "Mimic Ui Library",
    Keybind = Enum.KeyCode.LeftControl,
    Logo = 'http://www.roblox.com/asset/?id'
})

local TabFrame = Windows:NewTab({
    Title = "b1c2",
    Description = "Exploit"
})

local Section = TabFrame:NewSection({
    Title = "Part 1 Auto Pass",
    Position = "Left"
})

Section:NewButton({
    Title = "Auto Pass",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(65.12, 56.4, -1584.93)
    end
})

Section:NewButton({
    Title = "Auto Pass (2)",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(236.38, 101.94, -588.19)
    end
})

Section:NewButton({
    Title = "Part 3 Auto Pass",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(881.22, 74.06, -354.45)
    end
})

local ESPSection = TabFrame:NewSection({
    Title = "ESP Options",
    Position = "Left"
})

ESPSection:NewButton({
    Title = "ESP YenCoin",
    Callback = function()
        local RunService = game:GetService("RunService")
        local highlightedObjects = {}

        local function createESP(obj)
            local BillboardGui = Instance.new("BillboardGui")
            local TextLabel = Instance.new("TextLabel")

            BillboardGui.Parent = obj
            BillboardGui.AlwaysOnTop = true
            BillboardGui.Size = UDim2.new(0, 50, 0, 50)
            BillboardGui.StudsOffset = Vector3.new(0, 2, 0)

            TextLabel.Parent = BillboardGui
            TextLabel.BackgroundTransparency = 1
            TextLabel.Size = UDim2.new(1, 0, 1, 0)
            TextLabel.Text = "YenCoin"
            TextLabel.TextScaled = true
            TextLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            TextLabel.TextStrokeTransparency = 0.5

            highlightedObjects[obj] = true
        end

        local function handleNewObject(obj)
            if obj:IsA("MeshPart") and obj.Name == "YenCoin" and obj.Parent and obj.Parent.Name == "Yen" then
                if not highlightedObjects[obj] then
                    createESP(obj)
                end
            end
        end

        local function handleExistingObjects()
            for _, obj in ipairs(game.Workspace:GetDescendants()) do
                handleNewObject(obj)
            end
        end

        game.Workspace.ChildAdded:Connect(function(child)
            handleNewObject(child)
        end)

        RunService.Heartbeat:Connect(function()
            handleExistingObjects()
        end)

        handleExistingObjects()
    end
})
